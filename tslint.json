{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "max-line-length": {
        "options": [120]
      },
      "max-classes-per-file": [false],
      "quotemark": [true, "single"],
      "semicolon": [true, "never"],
      "trailing-comma": [true, {
        "multiline": "never",
        "singleline": "never"
      }],
      "object-literal-sort-keys": [false],
      "ordered-imports": [false]
    },
    "rulesDirectory": []
  }
  